set terminal pdf dpi 600
set output "fit.pdf"
set key outside
set grid
set yrange [-1.1:1.1]
set xlabel "$x$"
set ylabel "$y$"
set title "Neural Network curve fitting "
plot \
"out.txt"  index 1 using 1:2 w l lw 2 color Cyan  title "Fit" 	\
,"out.txt" index 0 using 1:2 w p pt 31 color BrickRed title "Data points" 				\
